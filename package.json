{
  "name": "nuxt-site",
  "version": "1.0.0",
  "description": "Nuxt-Proyect",
  "author": "Amaury Tobias Quiroz",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "dev": "node content/index.js && cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "node content/index.js && nuxt generate",
    "articles": "nodemon --ext md --watch content --watch content/index.js content/index.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@nuxtjs/axios": "^5.5.4",
    "bulma": "^0.7.5",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "front-matter-markdown": "^0.4.3",
    "frontmatter-markdown-loader": "^1.7.0",
    "highlight.js": "^9.15.8",
    "nuxt": "^2.8.1",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "vue-github-activity": "^2.1.1",
    "vue-lazyload": "^1.3.1",
    "vue2-animate": "^2.1.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/eslint-module": "^0.0.1",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0"
  }
}
